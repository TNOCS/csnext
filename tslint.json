{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "double"
    ],
    "max-line-length": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "variable-name": false,
    "allow-leading-underscore": true,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "arrow-parens": false,
    "object-literal-key-quotes": [
      false
    ]
  }
}