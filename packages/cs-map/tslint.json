{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single", "double"],
    "max-line-length": false,
    "naming-convention": [
      {"type": "default", "format": "camelCase", "leadingUnderscore": "allow", "trailingUnderscore": "allow"},
    ],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "arrow-parens": false,
    "object-literal-key-quotes": [
      false
    ]
  }
}
