{
    "name": "@csnext/cs-markdown",
    "version": "0.0.131",
    "description": "cs-markdown",
    "main": "./lib/csmarkdown.cs.js",
    "types": "./dist/src/index.d.ts",
    "url": "https://github.com/TNOCS/cs-markdown",
