{
    "private": true,
    "name": "csnext",
    "license": "MIT",
    "workspaces": [
        "packages/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/damylen/csnext.git"
    },
    "config": {
        "unsafe-perm": true
    },
    "scripts": {
        "postinstall": "lerna bootstrap && yarn run link",
        "dev": "lerna run --parallel dev",
        "clean": "lerna run clean && del-cli pnpm-lock.yaml pnpm-debug.log node_modules yarn.lock yarn-error.log package-lock.json",
        "link": "lerna run link",
        "build": "lerna run build",
        "unlink": "lerna run unlink",
        "build:parallel": "lerna run --parallel build",
        "dev:docs": "lerna run dev:docs",
        "build:docs": "lerna run build --scope cs-docs",
        "build:docker": "docker build -t damylen/csnext .",
        "publish:npm": "lerna publish --force-publish=*",
        "publish:beta": "npm build && lerna publish --force-publish=* --canary --cd-version patch",
        "watch": "tsc -b -w packages",
        "rebuild": "yarn run clean && yarn install && yarn run build",
        "test": "lerna run test",
        "lerna:version": "lerna version prerelease --preid beta",
        "lerna:publish": "lerna publish --npm-client yarn --npm-tag beta from-git --yes"
    },
    "devDependencies": {
        "core-js": "^2.6.7",
        "css-loader": "^3.2.0",
        "del-cli": "^2.0.0",
        "reflect-metadata": "^0.1.13",
        "vue": "^2.6.10",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.2.1",
        "vue-style-loader": "^4.1.2",
        "webpack": "^4.39.2",
        "webpack-bundle-analyzer": "^3.4.1",
        "yargs": "^13.3.0",
        "yarn": "^1.16.0",
        "sass": "1.22.10",
        "sass-loader": "^7.2.0",
        "fibers": "^4.0.1",
        "deepmerge": "4.0.0"
    },
    "dependencies": {}
}