{
  "lerna": "3.15",
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "allowBranch": "*",
      "conventionalCommits": true,
      "preid": "beta",
      "message": "chore(release): publish %s"
    }
  },
  "ignoreCommitters": false,
  "changelog": {
    "repo": "tnocs/csnext",
    "labels": {
      "feature": ":rocket: New Feature",
      "bug": ":bug: Bug Fix",
      "added": ":bug: Add"
    },
    "cacheDir": ".changelog"
  },
  "version": "0.0.121"
}
